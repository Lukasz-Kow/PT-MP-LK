<?xml version="1.0" encoding="utf-8"?><Database Name="BookShopDB" Class="BookShopDBLDataContext" xmlns="http://schemas.microsoft.com/linqtosql/dbml/2007">
  <Table Name="dbo.Books" Member="Books">
    <Type Name="Books">
      <Column Name="Id" Type="System.Int32" DbType="Int NOT NULL" IsPrimaryKey="true" CanBeNull="false" />
      <Column Name="Title" Type="System.String" DbType="NChar(100)" CanBeNull="true" />
      <Column Name="Author" Type="System.String" DbType="NChar(100)" CanBeNull="true" />
      <Column Name="Pages" Type="System.Decimal" DbType="Decimal(18,0)" CanBeNull="true" />
      <Column Name="ISBN" Type="System.String" DbType="NChar(100)" CanBeNull="true" />
      <Column Name="Publisher" Type="System.String" DbType="NChar(100)" CanBeNull="true" />
      <Column Name="Language" Type="System.String" DbType="NChar(100)" CanBeNull="true" />
      <Association Name="Books_Statuses" Member="Statuses" ThisKey="Id" OtherKey="BookId" Type="Statuses" />
    </Type>
  </Table>
  <Table Name="dbo.Customers" Member="Customers">
    <Type Name="Customers">
      <Column Name="Id" Type="System.Int32" DbType="Int NOT NULL" IsPrimaryKey="true" CanBeNull="false" />
      <Column Name="FirstName" Type="System.String" DbType="NChar(100)" CanBeNull="true" />
      <Column Name="LastName" Type="System.String" DbType="NChar(100)" CanBeNull="true" />
      <Column Name="Age" Type="System.Int32" DbType="Int" CanBeNull="true" />
      <Column Name="Address" Type="System.String" DbType="NChar(100)" CanBeNull="true" />
      <Column Name="City" Type="System.String" DbType="NChar(100)" CanBeNull="true" />
      <Association Name="Customers_Events" Member="Events" ThisKey="Id" OtherKey="CustomerId" Type="Events" />
    </Type>
  </Table>
  <Table Name="dbo.Events" Member="Events">
    <Type Name="Events">
      <Column Name="Id" Type="System.Int32" DbType="Int NOT NULL" IsPrimaryKey="true" CanBeNull="false" />
      <Column Name="StatusId" Type="System.Int32" DbType="Int NOT NULL" CanBeNull="false" />
      <Column Name="CustomerId" Type="System.Int32" DbType="Int NOT NULL" CanBeNull="false" />
      <Column Name="Time" Type="System.DateTime" DbType="DateTime NOT NULL" CanBeNull="false" />
      <Column Name="Type" Type="System.String" DbType="NChar(30)" CanBeNull="true" />
      <Column Name="Reason" Type="System.String" DbType="NChar(100)" CanBeNull="true" />
      <Column Name="Description" Type="System.String" DbType="NChar(100)" CanBeNull="true" />
      <Association Name="Customers_Events" Member="Customers" ThisKey="CustomerId" OtherKey="Id" Type="Customers" IsForeignKey="true" />
      <Association Name="Statuses_Events" Member="Statuses" ThisKey="StatusId" OtherKey="Id" Type="Statuses" IsForeignKey="true" />
    </Type>
  </Table>
  <Table Name="dbo.Statuses" Member="Statuses">
    <Type Name="Statuses">
      <Column Name="Id" Type="System.Int32" DbType="Int NOT NULL" IsPrimaryKey="true" CanBeNull="false" />
      <Column Name="BookId" Type="System.Int32" DbType="Int" CanBeNull="true" />
      <Column Name="Availability" Type="System.Boolean" DbType="Bit" CanBeNull="true" />
      <Association Name="Statuses_Events" Member="Events" ThisKey="Id" OtherKey="StatusId" Type="Events" />
      <Association Name="Books_Statuses" Member="Books" ThisKey="BookId" OtherKey="Id" Type="Books" IsForeignKey="true" />
    </Type>
  </Table>
</Database>